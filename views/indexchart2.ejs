                  <div class="row">
                    <div class="col s2"></div>
                        <div class="col s8">
                        
                              <div id="chart" style="height:550px; background-color: white;"></div>
                          

                                  
    <script>

var names='<%=names%>';
nara=new Array();
var t="";
   for(var i=0;i<names.length;i++){
       if(names[i]!='|')
       {t+=names[i];}
       else{
         nara.push(t);
         t="";
       }
     
     }
    
     var datap='<%=datai%>';
     var aram=new Array(); //outer array
     var ara=new Array();  //inner array
     var st="";
     var k=0;
     for(var i=0;i<datap.length;i++){
       if(datap[i]!=',')
       {st+=datap[i];}
       else{
         ara.push(st);
         k++;
         st="";
       }
       if(k==13){
         aram.push(ara);
         ara=new Array();
         k=0;
       }
     }

    //  aram.push(ara);
      // for(var g=0;g<nara.length;g++){
      //   aram[g][0]=nara[g];
      // }
   
  
        var chart = c3.generate({
            bindto: '#chart',
            axis: {
        x: {
            type: 'category',
            categories: ['Jan', 'Feb','Mar', 'Apr','May','Jun','Jul','Aug','Sep','Oct','Nov','Dec'],
        
        }
      },
            data: {
              columns:aram
            
            }
        });
        
      
      </script>
                     </div>
                           
                         
                        <div class="col s2"></div>
                      </div>


