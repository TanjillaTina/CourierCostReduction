<script>

    var datap='<%=datai%>';
    var aram=new Array(); //outer array
    var ara=new Array();  //inner array
    var st="";
    var k=0;
    for(var i=0;i<datap.length;i++){
      if(datap[i]!=',')
      {st+=datap[i];}
      else{
        ara.push(st);
        k++;
        st="";
      }
      if(k==13){
        aram.push(ara);
        ara=new Array();
        k=0;
      }
    }

     aram.push(ara);
 
  
       var chart = c3.generate({
           bindto: '#chart',
           axis: {
       x: {
           type: 'category',
           categories: ['Jan', 'Feb','Mar', 'Apr','May','Jun','Jul','Aug','Sep','Oct','Nov','Dec'],
       
       }
     },
           data: {
             columns:aram
           
           }
       });
       
   
     
                 
     </script>