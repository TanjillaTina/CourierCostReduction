
<!DOCTYPE html>
<html>
    <head>
            <meta charset="utf-8">
            <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
            <meta http-equiv="x-ua-compatible" content="ie=edge">
        <title>CourierCostReduction</title>


   <link href="/css/mo.css" rel="stylesheet">


   <!--Materialize starts here-->

      <!--Import Google Icon Font-->
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <!--Import materialize.css-->
      <link type="text/css" rel="stylesheet" href="/materialize/css/materialize.min.css"  media="screen,projection"/>
      

      <!--Let browser know website is optimized for mobile-->
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>


<!--Materialize ends here-->

 <!--Import jQuery before materialize.js-->
 <script type="text/javascript" src="/materialize/jque.min.js"></script>
 <script type="text/javascript" src="/materialize/js/materialize.min.js"></script>

    </head>
    <body>
          
      

   
      

            <div class="container-fluid fixed-top " style="background-color: #2d333a;">
           
                <nav>
                    <div class="nav-wrapper blue-grey darken-3">
                      <a href="#!" class="brand-logo"style="padding-left:60px; color:white;"><i class="material-icons prefix"style="color:white;">all_inbox</i>Courier Cost Reduction</a>
                      <ul class="right hide-on-med-and-down">
                        
                        <li><a href="/"><i class="material-icons right">exit_to_app</i>Back</a></li>
                      </ul>
        
                      <ul class="side-nav" id="mobile-demo">
                  
                          
                          <li><a href="/"><i class="material-icons right">exit_to_app</i>Back</a></li>
                     
                    </ul>
                    </div>
                  </nav>
            
          
               </div>
                      <!-- Fixed Top Menu div ends here -->
                      
          
      <br>
     
    
      <div class="row">
          <div class="col s0 m0"></div>
        <div class="col s12 m12">





            <div class="header">
                                    
                <h4 class="text-header">Flat Rate Suggestions for SQ Group</h4>
                <div class="progress">
                        <div class="indeterminate"></div>
                    </div>
                          
            </div>

            <% dates.forEach(function(dates){ %>
           
          <div class="card blue-grey darken-3">
                <div class="card-title">
                        <h4>Year <%= dates %></h4>
                    </div>
            
      <div class="row">
          
        <div class="row">
     

                <% may.forEach(function(may){ %>
                        

                    <% if (may._id.year==dates) { %>
                          <div class="col s12 m3">
                                <div class="card blue-grey darken-1">
                                      <div class="card-title">
                                          <h4>Month : <%=may._id.month%></h4>
                                      </div>
                                  <div class="card-content">
                                  
                                    <table class="bordered centered responsive-table">
                                          <tr>
                                            <th>Boxsize</th>
                                            <th>Percentage</th>
                                          </tr>

                                          <% boxes.forEach(function(box){ %>
                                            <% if (box.month==may._id.month && box.year==dates) { %>

                                              <tr>
                                                <td><%=box.boxsize%></td>
                                                <td><%=box.percen%>%</td>
                                              </tr>
                                            <% } %>
                                          <% }); %>
                                        
                                        </table>
                                  </div>
                                
                                </div>
                              </div>
                            
                     <% } %>

                    <% }); %>
    
              </div> 
   
         </div>
      
        

        </div>
   

        <% }); %>

        </div>
        <div class="col s0 m0"></div>
      </div>


    </body>
</html>
